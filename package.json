{
    "name": "webs",
    "version": "1.0.0",
    "description": "webs",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run migrate && npm run seed\" \"babel-node index.js\"",
        "dev": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
        "lint": "eslint . --ext .js",
        "lint:fix": "eslint --fix . --ext .js ",
        "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
        "db:create": "./node_modules/.bin/sequelize db:create",
        "db:create:test": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:create",
        "db:drop": "./node_modules/.bin/sequelize db:drop",
        "db:drop:test": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:drop",
        "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls && nyc --reporter=lcov --reporter=text-lcov npm test",
        "setup:test": "cross-env NODE_ENV=test npm run migrate:undo && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed",
        "migrate": "./node_modules/.bin/sequelize db:migrate",
        "seed": "node_modules/.bin/sequelize db:seed:all",
        "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
        "migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo:all",
        "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js"
    },
    "author": "Celestin Niyonsaba",
    "license": "ISC",
    "devDependencies": {
        "@types/algoliasearch": "^4.0.0",
        "@types/body-parser": "^1.19.0",
        "@types/express": "^4.17.6",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "nodemon": "^2.0.3"
    },
    "dependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/node": "^7.8.7",
        "@babel/polyfill": "^7.11.5",
        "@babel/preset-env": "^7.9.6",
        "@babel/register": "^7.9.0",
        "@hapi/joi": "^15.0.3",
        "algoliasearch": "^3.33.0",
        "axios": "^1.2.6",
        "babel-node": "0.0.1-security",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "concurrently": "^5.2.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "http-status": "^1.4.0",
        "joi": "^17.3.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "nodemailer": "^6.9.7",
        "path": "^0.12.7",
        "pg": "^8.0.3",
        "pg-hstore": "^2.3.3",
        "redis": "^3.0.2",
        "response-time": "^2.3.2",
        "sequelize": "^5.21.7",
        "sequelize-auto-migrations": "github:scimonster/sequelize-auto-migrations#a063aa6535a3f580623581bf866cef2d609531ba",
        "sequelize-cli": "^5.5.1",
        "sequelize-slugify": "^0.9.1",
        "stripe": "^8.55.0",
        "uuidv4": "^6.1.0"
    }
}
